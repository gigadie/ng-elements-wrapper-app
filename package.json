{
  "name": "ng-elements-wrapper-app",
  "version": "0.0.0",
  "config": {
    "env": "production"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server": "ng run ng-elements-wrapper-app:server --configuration=$npm_package_config_env",
    "build:browser": "ng run ng-elements-wrapper-app:browser --configuration=$npm_package_config_env",
    "config:ssr": "tsc --project ssr/tsconfig.ssr.$npm_package_config_env.json && node copyStatic.js --env=$npm_package_config_env",
    "build:ssr": "npm run build:browser && npm run build:server && npm run config:ssr",
    "serve:universal": "node ./dist/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/platform-server": "^8.1.1",
    "@angular/router": "~8.1.1",
    "@nguniversal/common": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "domino": "^2.1.3",
    "express": "^4.17.1",
    "localstorage-polyfill": "^1.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
